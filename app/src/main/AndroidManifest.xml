<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HappyActivity"
            android:label="@string/happy_label"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".NeutralActivity"
            android:label="@string/neutral_label"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".SadActivity"
            android:label="@string/sad_label"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ShopActivity"
            android:label="@string/shop_label"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".SongDetailsActivity"
            android:label="@string/song_details_label"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>